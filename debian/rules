#!/usr/bin/make -f
include /usr/share/dpkg/default.mk
include ./build_config.mk

%:
	dh $@ --parallel

override_dh_auto_configure:
	dh_auto_configure -- DEFINES+="VERSION=$(DEB_VERSION_UPSTREAM)"

override_dh_installman:
        dh_installman

show_license:
        cat Lincense

show_copyright:
        cat debian/copyright
